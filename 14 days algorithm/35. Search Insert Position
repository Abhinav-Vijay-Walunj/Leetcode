//https://leetcode.com/problems/search-insert-position/
class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        int s=0,e,mid;
        int n=nums.size();
        e=n-1;
        if(target>nums[n-1])
            return n;
        
        while(s<e)
        {
            mid=s+(e-s)/2;
            if(nums[mid] == target)
                return mid;
            else if(nums[mid]>target)
                e=mid-1;
            else
                s=mid+1;
        }
        return mid;
        
    }
};
